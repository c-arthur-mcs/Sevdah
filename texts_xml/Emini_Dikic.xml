<?xml version="1.0" encoding="UTF-8"?>

<?xml-model href="../Schema.rnc" type="application/relax-ng-compact-syntax"?>
<poem uid="od..004">
    <meta>
        <title>
            <title_bs>Емини</title_bs>
            <title_en>Emini</title_en>
        </title>
        <author type="poet" lat="Osman Đikić">Осман Ђикић</author>
        <year>1971</year>
        <desc>Text originally written in Bosnian cyrillic. Published in Sabrana Dela, 1971.</desc>
        <url>https://archive.org/details/dikic-osman-sabrana-djela-pjesme-i-drame-1971./page/77/mode/2up</url>
    </meta>
    <body>
        <lg>
            <l>
                <origin lang="slavic">Витка</origin>
                <origin lang="slavic">стаса,</origin>
                <origin lang="slavic">као</origin>
                <origin lang="slavic">јела</origin>
            </l>
            <l>
                <origin lang="slavic">У</origin>
                <origin lang="slavic">облаке</origin>
                <origin lang="slavic">што</origin>
                <origin lang="slavic">се</origin>
                <origin lang="slavic">вије,</origin>
            </l>
            <l>
                <origin lang="slavic">Обрашчића,</origin>
                <origin lang="slavic">на</origin>
                <origin lang="slavic">којима</origin>
            </l>
            <l>
                <origin lang="slavic">Расцвале</origin>
                <origin lang="slavic">се</origin>
                <origin lang="latin">руже</origin>
                <origin lang="slavic">двије.</origin>
            </l>
        </lg>

        <lg>
            <l>
                <origin lang="slavic">Очи</origin>
                <origin lang="slavic">твоје,</origin>
                <origin lang="slavic">очи</origin>
                <origin lang="slavic">чарне,</origin>
            </l>
            <l>
                <origin lang="slavic">Као</origin>
                <origin lang="slavic">јасне</origin>
                <origin lang="slavic">зв'језде</origin>
                <origin lang="slavic">сјају,</origin>
            </l>
            <l>
                <origin lang="slavic">Ја</origin>
                <origin lang="slavic">сам</origin>
                <origin lang="slavic">срећа</origin>
                <origin lang="slavic">по</origin>
                <origin lang="slavic">сто</origin>
                <origin lang="slavic">пута</origin>
            </l>
            <l>
                <origin lang="slavic">Кад</origin>
                <origin lang="slavic">ме</origin>
                <origin lang="slavic">мило</origin>
                <origin lang="slavic">погледају.</origin>
            </l>
        </lg>

        <lg>
            <l>
                <origin lang="slavic">Та</origin>
                <origin lang="slavic">дивна</origin>
                <origin lang="slavic">си</origin>
                <origin lang="slavic">ко</origin>
                <origin lang="turkic">хурија</origin>
            </l>
            <l>
                <origin lang="slavic">Понајљепша</origin>
                <origin lang="slavic">из</origin>
                <origin lang="turkic">џенета,</origin>
            </l>
            <l>
                <origin lang="slavic">Ти</origin>
                <origin lang="slavic">си</origin>
                <origin lang="slavic">мени</origin>
                <origin lang="turkic">Емо</origin>
                <origin lang="slavic">мила,</origin>
            </l>
            <l>
                <origin lang="slavic">Ц'јело</origin>
                <origin lang="slavic">благо</origin>
                <origin lang="slavic">овог</origin>
                <origin lang="slavic">св'јета.</origin>
            </l>
        </lg>

        <lg>
            <l>
                <origin lang="slavic">Па</origin>
                <origin lang="slavic">још</origin>
                <origin lang="slavic">само</origin>
                <origin lang="slavic">кад</origin>
                <origin lang="slavic">помислим,</origin>
            </l>
            <l>
                <origin lang="slavic">Да</origin>
                <origin lang="slavic">си</origin>
                <origin lang="slavic">ћерка</origin>
                <origin lang="slavic">рода</origin>
                <origin lang="slavic">славна,</origin>
            </l>
            <l>
                <origin lang="slavic">Што</origin>
                <origin lang="slavic">се</origin>
                <origin lang="slavic">није</origin>
                <origin lang="slavic">препадао</origin>
            </l>
            <l>
                <origin lang="slavic">Најжешћијех</origin>
                <origin lang="slavic">од</origin>
                <origin lang="turkic">душмана.</origin>
            </l>
        </lg>

        <lg>
            <l>
                <origin lang="n/a">О,</origin>
                <origin lang="slavic">ја</origin>
                <origin lang="slavic">задем</origin>
                <origin lang="slavic">да</origin>
                <origin lang="slavic">ти</origin>
                <origin lang="slavic">срце</origin>
            </l>
            <l>
                <origin lang="slavic">За</origin>
                <origin lang="slavic">дом</origin>
                <origin lang="slavic">мили</origin>
                <origin lang="slavic">ув'јек</origin>
                <origin lang="slavic">бије,</origin>
            </l>
            <l>
                <origin lang="slavic">Да</origin>
                <origin lang="slavic">ти</origin>
                <origin lang="slavic">око</origin>
                <origin lang="slavic">због</origin>
                <origin lang="slavic">невоља</origin>
            </l>
            <l>
                <origin lang="slavic">Његовијех</origin>
                <origin lang="slavic">сузе</origin>
                <origin lang="slavic">лије.</origin>
            </l>
        </lg>

        <lg>
            <l>
                <origin lang="slavic">Па</origin>
                <origin lang="turkic">зар</origin>
                <origin lang="slavic">има</origin>
                <origin lang="slavic">на</origin>
                <origin lang="slavic">свијету</origin>
            </l>
            <l>
                <origin lang="slavic">Срећнијега</origin>
                <origin lang="turkic">Аллах</origin>
                <origin lang="slavic">роба,</origin>
            </l>
            <l>
                <origin lang="slavic">Од</origin>
                <origin lang="turkic">Османа</origin>
                <origin lang="n/a">–</origin>
                <origin lang="slavic">усне</origin>
                <origin lang="slavic">твоје</origin>
            </l>
            <l>
                <origin lang="slavic">Што</origin>
                <origin lang="slavic">у</origin>
                <origin lang="slavic">свако</origin>
                <origin lang="slavic">људи</origin>
                <origin lang="slavic">доба!</origin>
            </l>
        </lg>
    </body>

</poem>
